<h1>Listing events</h1>
<div  class="well ">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>Id</th>
        <th>Priority</th>
        <th>Event date</th>
        <th>Description</th>
        <th>Hour</th>
        <th>Victims</th>
        <th>Arrested</th>
        <th>Suspects</th>
        <th>Vehicles</th>
        <th>Observations</th>
        <th>Backup file</th>
        <th>Weapons</th>
        <th>Drugs</th>
        <th>Source</th>
        <th>Analyst</th>
        <th>Zone</th>
        <th>Status event</th>
        <th>Crime</th>
        <th>Township</th>
        <th>Place</th>
        <th>Address</th>
        <th><%=t(:actions)%></th>
    </thead>
    <% @events.each do |event| %>
      <tbody>
        <tr>
          <td><%= event.id %></td>
          <td><%= event.priority %></td>
          <td><%= event.event_date %></td>
          <td><%= event.description %></td>
          <td><%= event.hour %></td>
          <td><%= event.victims %></td>
          <td><%= event.arrested %></td>
          <td><%= event.suspects %></td>
          <td><%= event.vehicles %></td>
          <td><%= event.observations %></td>
          <td><%= event.backup_file %></td>
          <td><%= event.weapons_id %></td>
          <td><%= event.drugs_id %></td>
          <td><%= event.source %></td>
          <td><%= event.analyst_id %></td>
          <td><%= event.zone_id %></td>
          <td><%= event.status_event_id %></td>
          <td><%= event.crime_id %></td>
          <td><%= event.township_id %></td>
          <td><%= event.place_id %></td>
          <td><%= event.address_id %></td>
          <td>
            <ul class="nav nav-pills">
              <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#" title='<%=t(:actions)%>'><i class="icon-cog"></i>&nbsp;<i class="icon-caret-down"></i></a>
                <ul class="dropdown-menu">
                  <li><%= link_to 'Show', event %></li>
                  <li><%= link_to 'Edit', edit_event_path(event) %></li>
                  <li><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></li>
                </ul>
              </li>
            </ul>
          </td>
        </tr>
      </tbody>
    <% end %>
  </table>

  <br />
  <%= link_to 'New Event', new_event_path,:class => "btn btn-primary" %>
